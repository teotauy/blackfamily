{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://blackfamily-production.up.railway.app/api/$1"
    }
  ],
  "routes": [
    {
      "src": "/admin-setup",
      "dest": "/admin-setup.html"
    },
    {
      "src": "/privacy-policy", 
      "dest": "/privacy-policy.html"
    },
    {
      "src": "/terms-of-service",
      "dest": "/terms-of-service.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 